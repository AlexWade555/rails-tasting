<h1>Create a new lesson</h1>
<div class=""></div>
<%= simple_form_for @lesson do |f| %>
    <%= f.input :title %>
    <%= f.input :category %>
    <%= f.input :rating %>
    <%= f.input :user_id, promt: 'select a user' %>
    <%= f.input :photo, as: :file %>
    <%= f.button :submit %>
<% end %>
<%= link_to "Back", lessons_path %>
